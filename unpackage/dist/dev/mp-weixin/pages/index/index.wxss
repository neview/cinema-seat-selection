page {
  background-color: #F8F8F8;
}
.sessions .film_info {
  margin-left: 30rpx;
}
.sessions .film_info .film_name {
  font-size: 34rpx;
  font-weight: bold;
}
.sessions .film_info .film_time {
  color: #666666;
  font-size: 28rpx;
  margin-top: 15rpx;
}
.sessions .hall_name {
  width: 100%;
  height: 136rpx;
  text-align: center;
  line-height: 136rpx;
  font-size: 22rpx;
  font-weight: 400;
  color: #9b9d9b;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  margin-top: 50rpx;
  z-index: 99;
}
.sessions .seating_map {
  width: 100vw;
  height: 600rpx;
}
.sessions .seating_map .rowList {
  display: flex;
  align-items: center;
  margin-top: 15rpx;
}
.sessions .seating_map .rowList .columnNoList {
  margin-left: 2rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.sessions .seating_map .rowList .columnNoList image {
  width: 100%;
  height: 100%;
}
.sessions .row_tips {
  width: 35rpx;
  background-color: #2C405A;
  color: #FFFFFF;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 20rpx;
  font-size: 24rpx;
  position: fixed;
  top: 285rpx;
  left: 30rpx;
  padding-top: 15rpx;
  transition: all 0.3s;
}
.sessions .row_tips ._span {
  display: inline-block;
  margin-bottom: 15rpx;
}
.render-nimation {
  -webkit-animation-name: circle-in-center;
          animation-name: circle-in-center;
  -webkit-animation-duration: .3s;
          animation-duration: .3s;
}
@-webkit-keyframes circle-in-center {
0% {
    opacity: 0;
    -webkit-transform: scale(0.2);
            transform: scale(0.2);
}
100% {
    opacity: 1;
    -webkit-transform: scale(1);
            transform: scale(1);
}
}
@keyframes circle-in-center {
0% {
    opacity: 0;
    -webkit-transform: scale(0.2);
            transform: scale(0.2);
}
100% {
    opacity: 1;
    -webkit-transform: scale(1);
            transform: scale(1);
}
}

